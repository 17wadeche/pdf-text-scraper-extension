{
  "manifest_version": 3,
  "name": "PDF Text Scraper",
  "version": "0.2.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "type":"module"
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["pdf.mjs", "pdf.worker.mjs", "styles"],
      "matches": ["<all_urls>"]
    }
  ]
}
